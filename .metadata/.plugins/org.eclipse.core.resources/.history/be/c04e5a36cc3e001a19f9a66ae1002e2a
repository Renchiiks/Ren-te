		getMyName();
		getMyName();
		getMyName();
		getMyName();
		System.out.println("++++++++++++++++++++++++++++++++++=");
		getMyCity();
	}
	public static void getMyName(){
		System.out.println("My name is Renate");
	}
	public static void getMyCity(){
		getMyName();
		System.out.println("My city is RÄ«ga");
	}
	public static void getIHaveCat(){
	
}
