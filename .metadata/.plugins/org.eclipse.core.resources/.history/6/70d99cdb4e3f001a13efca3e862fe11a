package lv.renate.player;

public class FootballPlayer {
	String name;
	String position;
	int shortNumber;
	boolean built;
	long height;
	int weight;
	
	public String getName(){
		return name;
	}
	public String getposition(){
		return position;	
	}
	public int getshirtName(){
		return shortNumber;	
	}
	public boolean getbuilt(){
		return built;	
	}
	public long getheight(){
		return height;	
	}
	public int getweight(){
		return weight;	
	}
	public void setName(String name){
		this.name = name;
	}
}
