package lv.renate;

public class LearnMethodsWithParams {

	
	public static void main(String[] args) {
		int result = getArea(30.2);
		System.out.println("Area: "+result);

	}
	//cast 
	public static int getArea(double radius){
		double result = Math.PI * radius * radius;
		return (int)result;
	}

}
