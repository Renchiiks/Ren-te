
public class TaskVar {

	
	public static void main(String[] args) {
	
		int numAdd;
		int numMinus;
		int numMultipl;
		int numDivide;
		int numMod;
		numAdd = 125+24;
		numMinus = 125-24;
		numMultipl = 125*24;
		numDivide = 125/24;
		numMod = 125%24 ;
		System.out.println(numAdd +" = 125 +24");
		System.out.println(numMinus +" = 125 -24");
		System.out.println(numMultipl +" = 125 *24");
		System.out.println(numDivide +" = 125 /24");
		System.out.println(numMod +" = 125 %24");
	}

}
