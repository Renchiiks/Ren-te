
public class ArraysSumForLoop {

	
	public static void main(String[] args) {
		int[] marks = { 23, 56, 6, 45, 67, 20, 34, 65 };
		
		//sum all values of arrays
		//print to console
		int result = 10;
		int testInt = 90;
		
		result = testInt;
		
		System.out.println("result value:" + result);
		System.out.println(marks[0]+marks[1]);
		
		

	}

}
