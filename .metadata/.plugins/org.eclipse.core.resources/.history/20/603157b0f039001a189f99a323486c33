public class ArraysPrimitiveDataType {

	public static void main(String[] args) {

		int[] arrays; // Declare array with type int
		// int arrays2[];//arÄ«
		arrays = new int[2];

		int[] arr = new int[10];

		int[] arrTest = new int[5];

		arrTest[0] = 10;
		arrTest[2] = 3;
		arrTest[4] = 4;
		System.out.println(arrTest[0] + " " + arrTest[1] + " " + arrTest[4]);

		double[] doubleArray = new double[3];
		doubleArray[0] = 0.5;
		doubleArray[1] = 1.5;
		doubleArray[2] = 7;

		System.out.println(doubleArray[0] + " " + doubleArray[1] + " "
				+ doubleArray[2]);
		
		char[] charArray = new char[2];
		
		charArray[0] = 22;
		charArray[1] = 'f';
		
		System.out.println(charArray+ " "+charArray);

	}

}
