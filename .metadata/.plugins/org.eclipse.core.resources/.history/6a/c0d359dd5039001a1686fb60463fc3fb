
public class PrimType {

	
	public static void main(String[] args) {
		//numeric data primitive type
		int integer =2;
		long longInteger= 234355L;
		short shortInteger = 234;
		byte byteInteger = 20;
		//Floating-Point type real numbers
		double doubleFloutNumber = 0.33d;
		float floatNumber = 0.23456f;
		//Characters
		char charSymbol = 'a';
		char charSymbolAsNumber = 33;
		System.out.println(charSymbol);
		System.out.println(charSymbolAsNumber);
		
		char R = 82;
		char e = 101;
		char n = 110;
		char a = 97;
		char t = 116;
		char e2 = 101;
		
		
		//Boolean
		boolean booleanTrueFalse = true;
		
	
		System.out.println("My name is" +R+e+n+a+t+e2);
		System.out.println("" +R+e+n+a+t+e2);
		System.out.println(booleanTrueFalse);
		

	}

}
